{
  "LogOptions": {
    "FileSizeLimitMegas": 1024, 
    "RetainedFileCountLimit": 5 
  },  
  "Service": {
    "Name": "license"
  },
  "EnvironmentSettings": {
    "Edition": "Private",
    "Version": "3.0.36018"
  },
  "ConnectionStrings": {
    "Redis": "redis:6379,password=REDISPASS,ssl=False,abortConnect=False"
  },
  "Console": {
    "EnabledRedis": true
  },
  "ApplySettings": {
    "ReceiveAccount": ""
  },
  "ServiceEndpoints": {
    "MetadataService": {
      "BaseAddress": "http://metadata-service",
      "MaxRetryCount": 3,
      "RetryIntervalMilliseconds": 1000
    },
    "AuditService": {
      "BaseAddress": "http://audit-service",
      "MaxRetryCount": 3,
      "RetryIntervalMilliseconds": 1000
    },
    "TenantService": {
      "BaseAddress": "http://tenant-service",
      "MaxRetryCount": 3,
      "RetryIntervalMilliseconds": 1000
    },
    "NotificationService": {
      "BaseAddress": "http://notification-service",
      "MaxRetryCount": 3,
      "RetryIntervalMilliseconds": 1000
    }
  }
}
